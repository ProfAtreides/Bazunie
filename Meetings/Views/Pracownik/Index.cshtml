@model IEnumerable<Meetings.Models.Pracownik>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Admin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImiePracownika)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NazwiskoPracownika)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stanowisko)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Haslo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDzialuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdFiliiNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Admin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImiePracownika)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NazwiskoPracownika)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stanowisko)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Haslo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDzialuNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdFiliiNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
